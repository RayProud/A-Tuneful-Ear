{
  "name": "A-Tuneful-Ear",
  "version": "0.1.0",
  "description": "An app for training your ears",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --color -p --progress --hide-modules --display-optimization-bailout",
    "start:dev": "webpack-dev-server",
    "start": "node index.js",
    "lint": "eslint --ext=jsx --ext=js --ignore-path .gitignore -- . ",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": "lint"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RayProud/A-Tuneful-Ear.git"
  },
  "author": "Ray Proud <stillerr@ya.ru>",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "cross-env": "5.1.3",
    "express": "4.16.2",
    "lodash": "4.17.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-cli": "^6.0.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "8.0.0-beta.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-redux-saga": "0.1.5",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "lint-staged": "6.1.1",
    "tap-spec": "4.1.1",
    "tape": "4.8.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
